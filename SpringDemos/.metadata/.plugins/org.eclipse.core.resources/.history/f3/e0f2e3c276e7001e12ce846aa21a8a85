package com.insurenceapp;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.CommandLineRunner;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

import com.insurenceapp.model.Insurence;
import com.insurenceapp.service.IInsurenceService;

@SpringBootApplication
public class SpringInsurenceappTestmybatisApplication implements CommandLineRunner{

	public static void main(String[] args) {
		SpringApplication.run(SpringInsurenceappTestmybatisApplication.class, args);
	}
	
	@Autowired
	IInsurenceService iInsurenceService;
	@Override
	public void run(String... args) throws Exception {
		// TODO Auto-generated method stub
		Insurence insurence=new Insurence(101, "M-101", "Tata", "life", 5, 10000)
		iInsurenceService.addInsurence(insurence);
	}

}
